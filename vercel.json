{
  "version": 2,
  "buildCommand": "npm run build:all || true",
  "devCommand": "npm start",
  "functions": {
    "server.js": {
      "runtime": "nodejs24.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
